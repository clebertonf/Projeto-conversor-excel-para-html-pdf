<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Table</title>
</head>
<body>
    <table>
        <thead>
            <% header.forEach(column => { %>
                <th> <%= column %> </th>
            <% }) %>
        </thead>
        <tbody>
            <% lines.forEach(line => { %>
                <tr>
                    <% line.forEach(element => { %>
                        <td> <%= element %> </td>
                    <% }) %>
                </tr>
            <% }) %>
        </tbody>
    </table>
    
</body>
</html>